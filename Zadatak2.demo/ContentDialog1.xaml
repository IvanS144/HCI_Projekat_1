<ContentDialog
    x:Class="Zadatak2.demo.ParametersDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Zadatak2.demo"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Title=""
    PrimaryButtonText="Back"
    SecondaryButtonText="Continue"
    PrimaryButtonClick="ContentDialog_PrimaryButtonClick"
    SecondaryButtonClick="ContentDialog_SecondaryButtonClick" Height="782" Width="1190">

    <Grid Margin="-9,-125,-58,-114">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <ComboBox x:Name="AssignentsComboBox" ItemsSource="{x:Bind assignmentComboBoxOptions}" Width="120" RenderTransformOrigin="0.494,0.499" Margin="10,163,0,0" Height="32"/>
        <ComboBox x:Name="CoresComboBox" ItemsSource="{x:Bind coresComboBoxOptions}" Grid.ColumnSpan="2" Margin="148,163,0,0" Width="120" Height="32"/>
        <TextBlock HorizontalAlignment="Left" Margin="10,117,0,0" Text="Max paralell" TextWrapping="Wrap" VerticalAlignment="Top" Width="113" Height="39"/>
        <TextBlock HorizontalAlignment="Left" Margin="148,117,0,0" Text="Cores per image" TextWrapping="Wrap" VerticalAlignment="Top" Width="118"/>
    </Grid>
</ContentDialog>
